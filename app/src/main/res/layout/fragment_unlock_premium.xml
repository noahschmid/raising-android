<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/overlay_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.UnlockPremiumFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView31"
            style="@style/RaisingLargeTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="tesetts"
            android:textColorHint="@color/raisingPrimary"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView30"
            style="@style/RaisingTitleNonBold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="asdfasdf"
            android:textColorHint="@color/raisingPrimary"
            app:layout_constraintBottom_toTopOf="@+id/button"
            app:layout_constraintEnd_toEndOf="@+id/textView31"
            app:layout_constraintStart_toStartOf="@+id/textView31"
            app:layout_constraintTop_toBottomOf="@+id/textView31" />

        <Button
            android:id="@+id/button"
            style="@style/ButtonRegistrationAndProfile"
            android:layout_width="0dp"
            android:layout_marginStart="@dimen/standardHorizontalMargin"
            android:layout_marginEnd="@dimen/standardHorizontalMargin"
            android:hint="Unlock now"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>